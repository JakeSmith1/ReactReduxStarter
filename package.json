{
  "name": "react-redux-starter",
  "version": "1.0.0",
  "description": "starter application with react, redux, node, and express",
  "main": "index.js",
  "scripts": {
    "prestart": "npm install",
    "clean": "rimraf dist",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js --progress --profile --colors",
    "build": "npm run clean && npm run build:webpack --progress --profile --colors",
    "dev": "NODE_ENV=development npm start",
    "start": "node devServer.js",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JakeSmith1/ReactReduxStarter.git"
  },
  "keywords": [
    "react",
    "redux",
    "webpack",
    "node",
    "express"
  ],
  "author": "Jake Smith",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JakeSmith1/ReactReduxStarter/issues"
  },
  "homepage": "https://github.com/JakeSmith1/ReactReduxStarter#readme",
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "nodemon": "^1.11.0",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "body-parser": "^1.15.2",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.14.0",
    "html-webpack-plugin": "^2.26.0",
    "node-sass": "^4.1.1",
    "path": "^0.12.7",
    "react": "^15.4.2",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^1.3.1",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-promise": "^0.5.3",
    "sass-loader": "^4.1.1",
    "webpack": "^1.14.0",
    "webpack-hot-middleware": "^2.15.0"
  }
